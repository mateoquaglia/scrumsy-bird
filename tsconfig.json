{
    "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  
  "include": ["js/**/*.js","test/**/*.ts","src/**/*.ts"],
  "exclude": ["node_modules"]
}

